section.pagination
    if (curPage !== 1 && previousPage !== 1)
        a(href="?page=1") 1
    if (hasPreviousPage)
        a(href=`?page=${previousPage}`) #{previousPage}
    a.active(href=`?page=${curPage}`) #{curPage}
    if (hasNextPage)
        a(href=`?page=${nextPage}`) #{nextPage}
    if (lastPage !== curPage && nextPage !== lastPage)
        a(href=`?page=${lastPage}`) #{lastPage} 